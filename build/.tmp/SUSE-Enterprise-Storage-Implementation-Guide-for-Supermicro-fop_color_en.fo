    <fo:block id="idm139983156307632"><fo:block><fo:block><fo:block margin-left="0pt" font-family="'Open Sans', OpenSans, sans-serif,'FreeSerif', serif" keep-together.within-column="always"><fo:block keep-with-next.within-column="always"><fo:block font-family="'Open Sans', OpenSans, sans-serif,'FreeSerif', serif" line-height="0.9 * 1.6" font-weight="normal" keep-with-next.within-column="always" space-before.minimum="2.8em" space-before.optimum="3.0em" space-before.maximum="3.2em" space-after.minimum="0.5em" space-after.optimum="0.7em" space-after.maximum="0.9em" text-align="start" start-indent="0pt"><fo:marker marker-class-name="section.head.marker.short">Software:</fo:marker><fo:block font-size="16pt"><fo:inline color="rgb(102,102,102)">1.3<fo:leader leader-pattern="space" leader-length="6mm"/></fo:inline><fo:inline color="rgb(67,146,57)">Software:</fo:inline></fo:block></fo:block></fo:block></fo:block></fo:block></fo:block>
      

      <fo:list-block id="idm139983156041904" space-before.optimum="1em" space-before.minimum="0.8em" space-before.maximum="1.2em" space-after.optimum="1em" space-after.minimum="0.8em" space-after.maximum="1.2em" provisional-label-separation="2mm" provisional-distance-between-starts="9.5mm"><fo:list-item id="idm139983156305392" space-before.optimum="0.8em" space-before.minimum="0.6em" space-before.maximum="1em" keep-with-next.within-column="always" keep-with-previous.within-column="auto"><fo:list-item-label end-indent="label-end()" text-align="end" color="rgb(67,146,57)"><fo:block><fo:instream-foreign-object content-height="0.4em" alignment-baseline="alphabetic" alignment-adjust="0.175em"><svg:svg xmlns:svg="http://www.w3.org/2000/svg" width="100" height="100"><svg:circle cx="50" cy="50" r="50" stroke="none" fill="rgb(67,146,57)"/></svg:svg></fo:instream-foreign-object></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
