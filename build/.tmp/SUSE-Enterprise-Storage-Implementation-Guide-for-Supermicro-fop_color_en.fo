    </fo:block>

    <fo:block id="idm139983156027824"><fo:block><fo:block><fo:block margin-left="0pt" font-family="'Open Sans', OpenSans, sans-serif,'FreeSerif', serif" keep-together.within-column="always"><fo:block keep-with-next.within-column="always"><fo:block font-family="'Open Sans', OpenSans, sans-serif,'FreeSerif', serif" line-height="0.9 * 1.6" font-weight="normal" keep-with-next.within-column="always" space-before.minimum="2.8em" space-before.optimum="3.0em" space-before.maximum="3.2em" space-after.minimum="0.5em" space-after.optimum="0.7em" space-after.maximum="0.9em" text-align="start" start-indent="0pt"><fo:marker marker-class-name="section.head.marker.short">Network/IP address scheme</fo:marker><fo:block font-size="16pt"><fo:inline color="rgb(102,102,102)">4.2<fo:leader leader-pattern="space" leader-length="6mm"/></fo:inline><fo:inline color="rgb(67,146,57)">Network/IP address scheme</fo:inline></fo:block></fo:block></fo:block></fo:block></fo:block></fo:block>
      

